{
    "name": "atari-turbo-basic",
    "displayName": "Atari TURBO BASIC XL Syntax",
    "description": "Landuage support for Atari Turbo BASIC XL",
    "publisher": "marianodominguez",
    "repository": {
        "url": "https://github.com/marianodominguez/atari-basic-vsc-extension.git",
        "type": "git"
    },
    "icon": "AtariBasicSyntax.png",
    "version": "0.3.0",
    "engines": {
        "vscode": "^1.51.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "atari.turbo.basic",
                "aliases": [
                    "Atari Turbo Basic",
                    "Atari TURBO BASIC",
                    "tbasic xl",
                    "TBASIC XL"
                ],
                "extensions": [
                    ".bas",
                    ".tb",
                    ".tur"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "atari.turbo.basic",
                "scopeName": "source.atari.turbo.basic",
                "path": "./syntaxes/AtariTBASIC.tmLanguage_4.json"
            }
        ]
    }
}
